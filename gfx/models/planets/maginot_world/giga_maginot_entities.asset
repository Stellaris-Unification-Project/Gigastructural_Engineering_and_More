entity = {
	name = "pc_giga_maginot_world_01_entity"
	pdxmesh = "planet_clouded_mesh"

	# surface ships
	attach = { planet = "pc_giga_maginot_world_surface_ships_entity" }
	locator = { name = "shipx2" rotation = { 45 0 180 } }
	attach = { shipx2 = "pc_giga_maginot_world_surface_ships_x2_entity" }
	locator = { name = "shipx3" rotation = { 90 90 90 } } # rotation seems irrelevant...?
	attach = { shipx3 = "pc_giga_maginot_world_surface_ships_x3_entity" }
	# locator shenanigans
	attach = { planet = "pc_giga_maginot_world_dummy_entity" }

	# surface texture
	meshsettings = {
		name = "planet_geosphereShape"
		shader = "PdxMeshPlanetEmissive"
		texture_diffuse = "giga_maginot_world_diffuse.dds"
		texture_normal = "giga_maginot_world_normal.dds"
		texture_specular = "giga_maginot_world_specular.dds"
	}
	# "cloud" texture (actually flying forts)
	meshsettings = {
		name = "clouds_geosphereShape"
		shader = "PdxMeshClouds"
		texture_diffuse = "clouds_maginot_01_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
	}	
	# cloud shadow texture
	meshsettings = {
		name = "clouds_shadow_geosphereShape"
		texture_diffuse = "clouds_maginot_shadow_01_diffuse.dds"
		texture_normal = "nonormal.dds"
		texture_specular = "nospec.dds"
	}

	default_state = "idle"
	playback_rate = 1.0 # Rotation is reversed
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }
	start_event = { trigger_once = yes sound = { soundeffect = "starbase_hum_stage_01" } }
	start_event = { trigger_once = yes sound = { soundeffect = "starbase_hum_stage_02" } }
	start_event = { trigger_once = yes sound = { soundeffect = "amb_city_planet" } }
	start_event = { trigger_once = yes sound = { soundeffect = "amb_city_planet_flyby" } }
	}
}

# locator shenanigans, makes stuff rotate with planet
# ACOT does this for the void sphere and it works
entity = {
	name = "pc_giga_maginot_world_dummy_entity"
	pdxmesh = "planet_no_pole_no_clouds_mesh"
	
	scale = 0.98

	# Rings
	locator = { name = "root"  rotation = { 0 0 0 } }
	locator = { name = "root2" rotation = { 0 0 0 } }
	# Orbital command station
	locator = { name = "top_station" position = { 0 6 0 } rotation = { 0 0 180 } }
	locator = { name = "bottom_station" position = { 0 -6 0 } rotation = { 0 0 0 } }
	# Surface greebles stolen from dyson sphere frame
	locator = { name = "greebles" rotation = { 0 0 0 } }
	# Planetary cannons
	locator = { name = "cannon_equator_right" position = { 4.2 0 0 } rotation = { 0 0 270 } }
	locator = { name = "cannon_halfnorth_right" position = { 3.6 -2.0 0 } rotation = { -183 90 -270 } }
	locator = { name = "cannon_halfsouth_right" position = { 3.6 2.0 0 } rotation = { 175 -90 90 } }
	# Opposite side
	locator = { name = "cannon_equator_left" position = { -4.2 0 0 } rotation = { 0 0 90 } }
	locator = { name = "cannon_halfnorth_left" position = { -3.6 -2.0 0 } rotation = { -175 90 -90 } }
	locator = { name = "cannon_halfsouth_left" position = { -3.6 2.0 0 } rotation = { 183 -90 270 } }
	# Orthogonal cannons
	locator = { name = "cannon_halfnorth_center" position = { 0 -1.3 4.2 } rotation = { -180 87 0 } }
	locator = { name = "cannon_halfsouth_center" position = { 0 1.3 4.2 } rotation = { 0 93 0 } }
	locator = { name = "cannon_halfnorth_back" position = { 0 -1.3 -4.2 } rotation = { -180 93 180 } }
	locator = { name = "cannon_halfsouth_back" position = { 0 1.3 -4.2 } rotation = { 0 87 180 } }

	# Rings
	attach = { root = "pc_giga_maginot_world_inner_planetary_ring_complete_entity" }
	attach = { root2 = "pc_giga_maginot_world_outer_planetary_ring_complete_entity" }
	# Orbital command station
	attach = { top_station = "pc_giga_maginot_world_orbital_top_entity" }
	attach = { bottom_station = "pc_giga_maginot_world_orbital_top_entity" }
	# Surface greebles
	attach = { greebles = "pc_giga_maginot_world_greeble_entity" }
	# Equatorial planetary cannons
	attach = { cannon_equator_right = "pc_giga_maginot_world_cannon_double_entity" }
	attach = { cannon_halfnorth_right = "pc_giga_maginot_world_kinetic_s_entity" }
	attach = { cannon_halfsouth_right = "pc_giga_maginot_world_kinetic_s_entity" }
	# Equatorial second group
	attach = { cannon_equator_left = "pc_giga_maginot_world_cannon_double_entity" }
	attach = { cannon_halfnorth_left = "pc_giga_maginot_world_kinetic_s_entity" }
	attach = { cannon_halfsouth_left = "pc_giga_maginot_world_kinetic_s_entity" }
	# Orthogonal cannons
	attach = { cannon_halfnorth_center = "pc_giga_maginot_world_laser_l_entity" }
	attach = { cannon_halfsouth_center = "pc_giga_maginot_world_laser_l_entity" }
	attach = { cannon_halfnorth_back = "pc_giga_maginot_world_laser_l_entity" }
	attach = { cannon_halfsouth_back = "pc_giga_maginot_world_laser_l_entity" }

	meshsettings = {
	name = "planet_geosphereShape"
	shader = "PdxMeshPlanetEmissive"
		texture_diffuse = "giga_maginot_world_diffuse.dds"
		texture_normal = "giga_maginot_world_normal.dds"
		texture_specular = "giga_maginot_world_specular.dds"
	}

	default_state = "idle"
	playback_rate = 1.0 # Rotation is reversed
	state = { name = "idle" animation = "idle" time_offset = { 0 100 }	}
}

